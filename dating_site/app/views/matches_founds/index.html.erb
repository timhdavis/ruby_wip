<p id="notice"><%= notice %></p>

<h1>Listing Matches Founds</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matches_founds.each do |matches_found| %>
      <tr>
        <td><%= matches_found.name %></td>
        <td><%= link_to 'Show', matches_found %></td>
        <td><%= link_to 'Edit', edit_matches_found_path(matches_found) %></td>
        <td><%= link_to 'Destroy', matches_found, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Matches found', new_matches_found_path %>
